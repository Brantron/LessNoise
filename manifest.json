{
  "manifest_version": 2,
  "name": "Less Noise",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*/*/commit/*",
        "*://github.com/*/*/pull/*/files"
      ],
      "js": ["jquery02.1.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon1.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "contentSettings",
    "http://*/*",
    "https://*/*"
  ]
}
